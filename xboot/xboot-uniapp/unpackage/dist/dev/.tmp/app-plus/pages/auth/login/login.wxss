.container {
	padding-top: 115px;
	position: relative;
	width: 100%;
	height: 100%;
	overflow: hidden;
	background: #fff;
}
.close {
	position: absolute;
	left: 40rpx;
	z-index: 9999;
	padding-top: var(--status-bar-height);
	top: 40rpx;
}
.wrap {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	padding: 0 80rpx 0 80rpx;
}
.title {
	font-size: 50rpx;
	font-weight: 500;
	padding-top: 60rpx;
}
.sub-title {
	font-size: 28rpx;
	color: #adacaf;
	padding-top: 10rpx;
}
.login-from {
	padding-top: 100rpx;
	padding-bottom: 100rpx;
}
.input {
	font-size: 32rpx;
	-webkit-box-flex: 1;
	-webkit-flex: 1;
	        flex: 1;
	display: inline-block;
	box-sizing: border-box;
	overflow: hidden;
}
.line-cell {
	padding: 27rpx 0;
	display: -webkit-flex;
	display: -webkit-box;
	display: flex;
	-webkiit-align-items: center;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	position: relative;
	box-sizing: border-box;
	overflow: hidden;
	border-bottom: 1rpx solid #c5cddb;
}
.top28 {
	margin-top: 28rpx;
	margin-bottom: 80rpx;
}
.bottom {
	position: fixed;
	z-index: 9999;




	bottom: env(safe-area-inset-bottom);

	text-align: center;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
}
.social {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	        align-items: center;
	margin-bottom: 40rpx;
}
.social image {
	width: 60rpx;
	height: 60rpx;
}
.right {
	margin-right: 60rpx;
}
.check {
	display: inline-block;
}
.tui-checkbox {
	margin-right: 10rpx;
}
.protocol {
	color: #333;
	font-size: 24rpx;
	text-align: center;
	width: 100%;
	margin-bottom: 10rpx;
}
.protocol-red {
	color: #C81816;
}

/*checkbox 整体大小  */
.tui-checkbox {
	width: 36rpx;
	height: 36rpx;
	border-radius: 50%;
}



/*checkbox 选项框大小  */
.tui-checkbox .wx-checkbox-input {
	width: 36rpx;
	height: 36rpx;
	border-radius: 50%;
}

/*checkbox选中后样式  */
.tui-checkbox .wx-checkbox-input.wx-checkbox-input-checked {
	background: #C81816;
	width: 38rpx !important;
	height: 38rpx !important;
	border: none;
}

/*checkbox选中后图标样式  */
.tui-checkbox .wx-checkbox-input.wx-checkbox-input-checked::before {
	width: 30rpx !important;
	height: 30rpx !important;
	line-height: 30rpx;
	text-align: center;
	font-size: 20rpx;
	color: #fff;
	background: transparent;
	transform: translate(-50%, -50%);
	-webkit-transform: translate(-50%, -50%);
}
































